# Local Development Environment Variables

# Authentication (required for NextAuth)
AUTH_SECRET="" # Generated by create-t3-app

# Spotify OAuth (required for Spotify authentication)
SPOTIFY_CLIENT_ID=""
SPOTIFY_CLIENT_SECRET=""

# Database Connection (local Docker container on port 5433)
DATABASE_URL="postgresql://postgres:postgres@localhost:5433/spotifyFollowManager"

# NextAuth Configuration (local development URL)
NEXTAUTH_URL="http://localhost:3000"

# Flag to disable static mode during local development
AMPLIFY_DISABLE_STATIC=true

ENV_NAME="local"
